{
  "name": "horror-ai-transformer",
  "version": "1.0.0",
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"electron .\"",
    "electron:build": "electron-builder"
  },
  "build": {
    "appId": "com.horror.ai.transformer",
    "productName": "HorrorAI",
    "files": ["dist/**/*", "electron/**/*"],
    "directories": { "buildResources": "assets" },
    "win": { "target": "nsis" }
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.13.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.2"
  }
}
