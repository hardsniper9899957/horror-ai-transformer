{
  "name": "horror-ai-transformer",
  "version": "1.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"electron .\"",
    "electron:build": "electron-builder"
  },
  "build": {
    "appId": "com.horror.ai.transformer",
    "productName": "HorrorAI",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "concurrently": "^8.2.0",
    "vite": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
